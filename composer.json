{
    "name": "upline/nova-link-field",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Aleksei Orlov",
            "email": "ysywix@gmail.com",
            "homepage": "https://github.com/vaiil"
        }
    ],
    "require": {
        "php": "^7.3|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Upline\\NovaLinkField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Upline\\NovaLinkField\\FieldServiceProvider"
            ]
        }
